{
  "functions": {
    "api/proxy/gemini": "api/proxy/gemini.js"
    "api/404": "api/404.js"
  },
  "rewrites": [
    {
      "source": "/api/proxy/gemini",
      "destination": "/api/gemini.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/404.js"
    },
    {
      "source": "/(.*)",
      "destination": "/api/404.js"
    }
  ],
  "regions": ["sin"]
}
